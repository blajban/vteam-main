{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off",
        "no-return-await": "off",
        "arrow-body-style": "off",
        "no-unused-vars": "off",
        "radix": "off"
    },
    "plugins": ["jest"]
}
